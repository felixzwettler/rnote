<?xml version="1.0" encoding="UTF-8"?>
<!-- ### IconPicker ### -->
<interface>
  <template class="IconPicker" parent="GtkWidget">
    <property name="layout-manager">
      <object class="GtkBinLayout" />
    </property>
    <style>
      <class name="iconpicker" />
    </style>
    <child>
      <object class="GtkScrolledWindow">
        <property name="propagate-natural-width">true</property>
        <property name="propagate-natural-height">true</property>
        <property name="max-content-height">240</property>
        <child>
          <object class="GtkGridView" id="gridview">
            <property name="max-columns">7</property>
            <style>
              <class name="view" />
              <class name="iconpicker_gridview" />
            </style>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>